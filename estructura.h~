#ifndef std
	#include<stdio.h>
	#include<string.h>
#endif

#ifndef largo
	#define largo 300
#endif

#ifndef estructuras
	#define estructuras
	typedef struct F{
		char nombre[largo];
		int tipo;
		int num;
		lista hijos;
	} file;

	typedef struct C{
		void *cont;
		C *next,*prev;
	}caja;

	typedef struct {
		caja *first;
		int tam;
	} lista;


#endif
